<header id="header">
    Food Recipe
</header>



<body id="HomePage">
    <div>
        <nav>
            
            <a href="/" target="_self">
            <span>Home</span></a>
            <a href="/upload" target="_self">
            <span>Upload</span></a>
            <a href="/recipe" target="_self">
            <span>Recipe</span></a>
            <a href="/result" target="_self">
                <span>Result</span></a>

            <div class="search">
                <form action="/search" method="POST">
                    <input type="text" name="searchTerm" class="searchTerm" placeholder="What are you looking for?">
                    <input name="withoutRedirect" value="true" style="display:none">
                </form>
            </div>

        </nav>
    </div>

    <div>
        <table class="recipeform">
            <caption>Recipe List</caption>
            <tr>
                <th colspan="2">Title</th>
            </tr>

            <tr>
                <td colspan="2">$title</td>
            </tr>

            <tr>
                <th colspan="2">Image</th>
            </tr>

            <tr>
                <td colspan="2">
                    <$img>
                    </$img>
                </td>
            </tr>

            <tr>
                <th colspan="2">Introduction:</th>
            </tr>

            <tr>
                <td colspan="2">$introduction</td>
            </tr>

            <tr>
                <th>Level</th>
                <th>Yield</th>
            </tr>

            <tr>
                <td>$level</td>
                <td>$yield</td>
            </tr>

            <tr>
                <th>Ingredient</th>
                <th>Direction</th>
            </tr>

            <tr>
                <th>$ingredient</th>
                <th>$direction</th>
            </tr>
        </table>
    </div>
</body>


